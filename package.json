  },
  "author": {
    "name": "Rob Rohan",
    "email": "rob@therohans.com",
    "url": "https://robrohan.com"
  },
  "scripts": {
    "build": "npx parcel build src/caster.js",
    "start": "npx parcel serve index.html",
    "test": ""
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "glslify-bundle": "^5.1.1",
    "glslify-deps": "^1.3.1",
    "parcel-bundler": "^1.12.3"
  },
  "eslintConfig": {
    "extends": "google",
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "es6": true
    },
    "overrides": [
      {
        "files": [
          "src/*.js"
        ],
        "excludedFiles": "*.test.js"
      }
    ],
    "rules": {
      "require-jsdoc": "off"
    }
  }
}
