	"license": "SEE LICENSE IN LICENSE",
	"repository": {
		"type": "git",
		"url": "git@github.com:TheRohans/caster.git"
	},
	"author": {
		"name": "Rob Rohan",
		"email": "rob@therohans.com",
		"url": "https://robrohan.com"
	},
	"scripts": {
		"install:deps": "npm install --no-optional",
		"build": "NODE_ENV=production node node_modules/.bin/webpack --display-error-details --progress -p",
		"start": "NODE_ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js -d --progress --port 8080 --host 0.0.0.0",
		"test": ""
	},
	"dependencies": {},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"css-loader": "^3.0.0",
		"eslint": "^5.16.0",
		"eslint-config-google": "^0.13.0",
		"file-loader": "^0.11.2",
		"html-loader": "^0.5.1",
		"postcss-loader": "^2.0.6",
		"raw-loader": "^0.5.1",
		"webpack": "^4.19.1",
		"webpack-cli": "^3.1.0",
		"webpack-dev-server": "^3.1.8"
  },
  "eslintConfig": {
      "extends": "google",
      "parserOptions": {
        "ecmaVersion": 2017
      },
      "env": {
        "es6": true
      },
      "overrides": [{
        "files": ["src/*.js"],
        "excludedFiles": "*.test.js"
      }],
      "rules": {
        "require-jsdoc": "off"
      }
  }
}
